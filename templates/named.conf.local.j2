//

// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";
acl internal {
    127.0.0.0/8;
};

{% for item in bind_rfc2136_keys %}
key "{{ item.name }}" {
    algorithm {{ item.algo }};
    secret "{{ item.value }}";
};
{% endfor %}
